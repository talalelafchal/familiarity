d$time[d$opt=='on' & d$benchmark=='bloat'] + 190
d$time[d$opt=='on' & d$benchmark=='pmd' & d$gc=='Immix'] =
d$time[d$opt=='on' & d$benchmark=='pmd' & d$gc=='Immix'] + 600
